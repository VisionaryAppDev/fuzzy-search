{
    "name": "fuzzy-search",
    "displayName": "fuzzy-search",
    "description": "",
    "version": "0.0.1",
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.fuzzy-search"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "commands": [{
            "command": "extension.fuzzy-search",
            "title": "fuzzy-search"
        }],
         "configuration": {
             "title": "Ace Jump",
             "type": "object",
             "properties": {
                 "fuzzy-search.highlight": {
                     "type": "boolean",
                     "default": "true"
                 },
                 "fuzzy-search.border":{
                    "type":"boolean",
                    "default": "true"
                 },
                "fuzzy-search.border-dark-them-color": {
                    "type": "string",
                    "default": "lightblue"
                },
                "fuzzy-search.border-ruler-color": {
                    "type": "string",
                    "default": "blue"
                },
                 "fuzzy-search.border-light-them-color": {
                     "type": "string",
                     "default": "darkblue"
                 },
                 "fuzzy-search.relocation": {
                     "type": "string",
                     "enum": [
                         "top",
                         "center",
                         ""
                     ],
                     "default": "center"
                 }
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^3.1.4",
        "vscode": "^1.1.25",
        "tslint": "^5.8.0",
        "@types/node": "^8.10.25",
        "@types/mocha": "^2.2.42",
        "fuzzy": "*"
    }
}